<Application xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:local="using:MobileBandSync" 
             x:Class="MobileBandSync.App">
    <Application.Resources>
        <ResourceDictionary>
            <!--ThemeDictionaries-->
                <!--ResourceDictionary-->
                    <ResourceDictionary x:Key="Default">
                        <ImageBrush x:Key="HubBackgroundImageBrush" Stretch="None" 
                                    ImageSource="Assets/HubBackground.png" />
                    </ResourceDictionary>
                    <ResourceDictionary x:Key="HighContrast">
                        <ImageBrush x:Key="HubBackgroundImageBrush" Stretch="None" ImageSource="Null"/>
                    </ResourceDictionary>
                <!--/ResourceDictionary-->
            <!--/ThemeDictionaries-->
        </ResourceDictionary>
    </Application.Resources>
</Application>

